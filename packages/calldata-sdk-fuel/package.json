{
  "name": "@1delta/calldata-sdk-fuel",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.52",
  "description": "⚒️ An SDK for building calldata for 1delta smart contracts",
  "files": [
    "dist"
  ],
  "keywords": [
    "dex"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@1delta/calldatalib": "0.0.33",
    "fuels": "^0.101.0",
    "lodash": "^4.17.21",
    "tiny-invariant": "^1.3.3",
    "tiny-warning": "^1.0.3",
    "toformat": "^2.0.0",
    "viem": "^2.30.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.17",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "peerDependenciesMeta": {
    "test-routing": {
      "optional": true
    },
    "@1delta/universal-router": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}

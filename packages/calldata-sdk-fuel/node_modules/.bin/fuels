#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/achthar/calldata-sdk/node_modules/.pnpm/fuels@0.101.2_vitest@3.2.3_@types+node@22.15.32_/node_modules/fuels/node_modules:/home/achthar/calldata-sdk/node_modules/.pnpm/fuels@0.101.2_vitest@3.2.3_@types+node@22.15.32_/node_modules:/home/achthar/calldata-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/achthar/calldata-sdk/node_modules/.pnpm/fuels@0.101.2_vitest@3.2.3_@types+node@22.15.32_/node_modules/fuels/node_modules:/home/achthar/calldata-sdk/node_modules/.pnpm/fuels@0.101.2_vitest@3.2.3_@types+node@22.15.32_/node_modules:/home/achthar/calldata-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fuels/fuels.js" "$@"
else
  exec node  "$basedir/../fuels/fuels.js" "$@"
fi

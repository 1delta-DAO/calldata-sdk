{
  "name": "@1delta/data-sdk",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.1",
  "description": "Hold and initialize lending protocol data",
  "files": [
    "dist"
  ],
  "keywords": [
    "dex"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependenciesMeta": {
    "test-routing": {
      "optional": true
    },
    "@1delta/universal-router": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}

{
  "name": "calldata-sdk",
  "version": "0.0.1",
  "packageManager": "pnpm@10.10.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r run dev",
    "test": "pnpm -r run test",
    "clean": "pnpm -r exec rimraf dist && pnpm store prune && rm -rf node_modules && rm -rf packages/**/node_modules"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "lodash": "^4.17.21",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@1delta/chain-registry": "^0.0.1",
    "@1delta/data-sdk": "0.0.8",
    "@1delta/dex-registry": "^0.0.73",
    "@1delta/lender-registry": "^0.0.2",
    "@1delta/type-sdk": "^0.0.3",
    "@types/lodash": "^4.17.20",
    "axios": "^1.11.0"
  },
  "pnpm": {
    "overrides": {
      "@1delta/dex-registry": "^0.0.72"
    }
  }
}
